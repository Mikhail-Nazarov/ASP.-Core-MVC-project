@using MyCompany.Domain.Entities
@model ServiceItem
@{
    ViewBag.Title = Model.MetaTitle;
    ViewBag.Description = Model.MetaDescription;
    ViewBag.Keywords = Model.MetaKeywords;
}
<div>
    <h2>
        @Model.Title
    </h2>
    <h2>
        @Model.Subtitle
    </h2>
    <div>
        <img src="~/images/@Model.TitleImagePath" class="img-block" alt="" />
    </div>
</div>
<div>
    @Html.Raw(Model.Text)
</div>